{
  "name": "spatulafoods",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "codegen": "graphql-codegen --config codegen.yml",
    "db:push": "yarn prisma db push",
    "db:seed": "yarn prisma db seed",
    "dev": "next dev",
    "postinstall": "yarn prisma:generate",
    "lint": "eslint src/**/*.{ts,tsx} ./*.js",
    "lint:fix": "yarn lint --fix",
    "prisma:generate": " prisma generate",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "start": "next start"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "eslint"
    ],
    "package.json": [
      "prettier --write",
      "sort-package-json"
    ],
    "*.json": [
      "prettier --write"
    ],
    ".eslintrc": [
      "prettier --write"
    ],
    ".pretierrc": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@graphql-codegen/typescript-urql": "^3.4.1",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@kunukn/react-collapse": "^2.2.9",
    "@neojp/tailwindcss-important-variant": "^1.0.1",
    "@next-auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^3.6.0",
    "@react-aria/button": "^3.3.4",
    "@react-aria/utils": "^3.11.0",
    "@styled-icons/remix-editor": "^10.33.0",
    "@themesberg/flowbite": "^1.2.0",
    "@tippyjs/react": "^4.2.6",
    "@tiptap/extension-blockquote": "^2.0.0-beta.26",
    "@tiptap/extension-text-align": "^2.0.0-beta.29",
    "@tiptap/extension-underline": "^2.0.0-beta.22",
    "@tiptap/react": "^2.0.0-beta.101",
    "@tiptap/starter-kit": "^2.0.0-beta.163",
    "@types/eslint": "^8.2.1",
    "@types/tailwindcss": "^2.2.4",
    "apollo-server-micro": "^2.19.0",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "dotenv": "^10.0.0",
    "graphql": "15.0.0",
    "lodash": "^4.17.21",
    "micro-cors": "^0.1.1",
    "next": "11.0.1",
    "next-auth": "^4.0.5",
    "next-themes": "^0.0.15",
    "nexus": "^1.2.0-next.17",
    "nexus-prisma": "^0.35.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-select": "^5.2.1",
    "shopify-api-node": "^3.8.1",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.45.0",
    "urql": "^2.0.6"
  },
  "devDependencies": {
    "@dotenv/cli": "^0.5.1",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/urql-introspection": "2.1.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^17.0.0",
    "@types/react": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "autoprefixer": "^10.3.1",
    "eslint": "^8.5.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "postcss": "^8.3.5",
    "prettier": "^2.5.1",
    "prisma": "^3.6.0",
    "sort-package-json": "^1.53.1",
    "tailwindcss": "^3.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
